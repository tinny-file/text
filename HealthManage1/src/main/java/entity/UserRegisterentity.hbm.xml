<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.UserRegisterentity" table="user_register" schema="health">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="username">
            <column name="username" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(16)" length="16" not-null="true"/>
        </property>
        <property name="age">
            <column name="age" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="tel">
            <column name="tel" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="identity">
            <column name="identity" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="card">
            <column name="card" sql-type="int(1)" not-null="true"/>
        </property>
        <property name="sex">
            <column name="sex" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>